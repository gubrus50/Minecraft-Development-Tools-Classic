<CUSTOM-BAR-SCRIPTS> <!-- Use this block to hide -->
	
	<SCRIPT src="scripts/custom-bar-function.js"></SCRIPT>
	<SCRIPT name="Close Window" type="text/vbscript">

	Sub closeWindow()

		Window.Close
		
	End Sub

	</SCRIPT>
	
	<SCRIPT name="Minimize Window" type="text/javascript">

	function _jsMinWin() {
		try {
			HHCtrlMinimizeWindowObject.Click();
		}
		catch (err) {
			alert(err.message);
		}
	}

	</SCRIPT>

	<OBJECT hidden="hidden" id="HHCtrlMinimizeWindowObject" classid="clsid:adb880a6-d8ff-11cf-9377-00aa003b7a11">
		<PARAM name="command" value="minimize" />
	</OBJECT>

</CUSTOM-BAR-SCRIPTS>



<SCRIPT type="text/javascript">

window.resizeTo(988,518);

// Disables hilighting
$(document).ready(function() {
	document.onselectstart = function() {return false}; // for ie
	document.onmousedown = function() {return false}; // for mozilla
});

</SCRIPT>



<STYLE>

body {
	background-image: url("images/background.jpg"); 
	background-repeat: no-repeat;
	-webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
}

a {cursor: default}
p {cursor: default}

#display {
	position: absolute;
	top: 32px;
	left: 1.2px; 
	width: 99.8%;
	height: 100%;
}

#close {
	position: absolute;
	right: 2px;
	top: 2px;
	width: 10px;
	height: 20px;
	opacity: 0.5;
}
#close:hover {opacity: 0.8}
#close:before {transform: rotate(45deg)}
#close:after {transform: rotate(-45deg)}
#close:before, #close:after {
	position: absolute;
	left: 0;
	content: ' ';
	height: 15px;
	width: 2px;
	background-color: #fff;
}

#hide {
	position: absolute;
	right: 2px;
	top: 8px;
	width: 10px;
	height: 20px;
	opacity: 0.5;
}
#hide:hover {opacity: 0.8}
#hide:before {transform: rotate(90deg)}
#hide:before {
	position: absolute;
	left: 0;
	content: ' ';
	height: 12px;
	width: 2px;
	background-color: #fff;
}

#return {
	border-width: 6px 10px 6px 0px;
	border-color: transparent #fff;
	font-family: Calibri;
	border-style: solid;
	position: absolute;
	opacity: 0.8;
	height: 0px;
	width: 0px;
	top: -6px;
	left: 11px;
}

#return {opacity: 0.5}
#return:hover {opacity: 0.8}

</STYLE>

</HEAD>
<BODY scroll="no">
<SCRIPT type="text/javascript">
// Disables crtl+scroll function
var obj=document.body;
if(obj.addEventListener)
{
    obj.addEventListener('DOMMouseScroll',mouseWheel,false);
    obj.addEventListener("mousewheel",mouseWheel,false);
}
else obj.onmousewheel=mouseWheel;

function mouseWheel(e)
{
    // disabling
    e=e?e:window.event;
    if(e.ctrlKey)
    {
        if(e.preventDefault) e.preventDefault();
        else e.returnValue=false;
        return false;
    }
}
</SCRIPT>
<DIV style="margin-left: -6.6px; margin-top: -8px; width: 984px; height: 516px; border: 1px solid #2b2b33">
