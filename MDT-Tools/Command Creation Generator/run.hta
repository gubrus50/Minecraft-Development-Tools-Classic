<script type="text/vbscript">

Function updateConfig()

	Dim objWSS, objFSO, objFolder, objFile

	' Sets variables
	'
	Set objWSS = CreateObject("WScript.Shell")
	objFolderCCG = objWSS.ExpandEnvironmentStrings("%APPDATA%\MDT-Tools\CommandCreationGenerator\")
	'
	Set objFSO = CreateObject("Scripting.FileSystemObject")
	Set objFolder = objFSO.GetFolder(objFolderCCG & "projects")
	Set config = objFSO.OpenTextFile(objFolderCCG & "scripts\cce_project_config.js",2,true)

	' It will edit or create if config does not exist
	'
	config.Write "jQuery( document ).ready( function() {"
	config.WriteBlankLines(2)

	' Loops through projects folder
	'
	For Each objFile In objFolder.Files

		' Appends script which will import the project to the application
		'
		Project = ("	project=""" & objFile.Name & """, imp();")
		config.Write Project
		config.WriteBlankLines(1)

	Next

	' Appends the ending line for the config
	'
	config.WriteBlankLines(1)
	config.Write "});"

END Function
</script>

